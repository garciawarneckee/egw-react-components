{
  "name": "egw-react-components",
  "version": "0.1.4",
  "description": "Custom React Components ",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build && mkdir build",
    "build": "npm-run-all clean build:css build:js",
    "build:js": "babel lib -d build --copy-files",
    "build:css": "node-sass ./lib -o build/",
    "lint": "eslint lib/**; exit 0",
    "lint:watch": "esw -w lib/**",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garciawarneckee/egw-react-components.git"
  },
  "keywords": [
    "react",
    "ui",
    "components"
  ],
  "author": "Esteban Garcia Warnecke <garciawarneckee@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/garciawarneckee/egw-react-components/issues"
  },
  "homepage": "https://github.com/garciawarneckee/egw-react-components#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.10",
    "@fortawesome/free-solid-svg-icons": "^5.6.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@storybook/react": "^4.1.2",
    "chai": "^4.2.0",
    "classnames": "^2.2.6",
    "polished": "^1.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-draggable": "^3.0.5",
    "styled-components": "^3.4.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^3.1.5",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>test/setupTests.js"
  }
}
